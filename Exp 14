#include <stdio.h>
#include <string.h>

char file[10][10]; int n=0;
int main(){
 int c; char name[10];
 while(1){
  printf("1.Create 2.Delete 3.Search 4.Display 5.Exit: ");
  scanf("%d",&c);
  if(c==1){scanf("%s",file[n++]);}
  else if(c==2){scanf("%s",name);
    for(int i=0;i<n;i++) if(!strcmp(file[i],name)) strcpy(file[i],file[--n]);
  }
  else if(c==3){scanf("%s",name);
    for(int i=0;i<n;i++) if(!strcmp(file[i],name)) printf("Found\n");
  }
  else if(c==4){for(int i=0;i<n;i++) printf("%s\n",file[i]);}
  else return 0;
 }
}
